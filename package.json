{
  "name": "replay-chess",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "test:e2e": "pnpm --filter web exec playwright test",
    "test:e2e:ui": "pnpm --filter web exec playwright test --ui",
    "test:e2e:headed": "pnpm --filter web exec playwright test --headed"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=22"
  }
}
